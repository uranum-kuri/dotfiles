export CC=gcc
export CXX=g++

export RUSTC_WRAPPER="$HOME/.cargo/bin/sccache"

export PYENV_ROOT="$HOME/.pyenv"
export NODENV_ROOT="$HOME/.nodenv"
export GOENV_ROOT="$HOME/.goenv"
